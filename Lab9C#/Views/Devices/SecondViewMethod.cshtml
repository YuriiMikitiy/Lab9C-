
@{
    ViewBag.Title = "SecondViewMethod";
}

<h2>Відсортовоний список приладів</h2>

<table class="table" style="width:20%">
    <thead>
    <tr>
        <th class="text-center">Назва</th>
    </tr>
    </thead>
    <tbody>
        @foreach (var vegetable in Model)
        {
            <tr class="text-center">
                <td>@vegetable</td>
            </tr>
        }
    </tbody>
</table>
